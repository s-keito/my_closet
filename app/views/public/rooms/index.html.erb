<div class="container">
  <div class="row">
    <div class="col-2"></div>
     <div class="col-8 my-5">

      <table class="table table-striped">
        <tbody>
          <tr>
            <td><% @another_entries.each do |entry| %>
              <%= link_to room_path(entry.room) do %>
                <div class="chat-avatar">
                  <%= image_tag entry.user.get_user_image, size: "100x100" %>
                  <%= entry.user.name %>
                </div>
              <% end %>
            <% end %></td>
          </tr>
        </tbody>
      </table>

     </div>
    <div class="col-2"></div>
  </div>
</div>
