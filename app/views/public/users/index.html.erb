<div class="container">
  <div class="row">
    <table class="table">
      <thead class="thead-dark">
        <tr>
          <th>image</th>
          <th>name</th>
          <th>follow</th>
          <th>follower</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td>
           <%= image_tag user.get_user_image, size:'50x50' %>
          </td>
          <td>
           <%= link_to user.name  %>
          </td>
          <td>#</td>
          <td>#</td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
